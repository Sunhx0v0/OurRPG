<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="472.0" prefWidth="430.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.registcontroller">
   <children>
      <TextField fx:id="name" layoutX="114.0" layoutY="214.0" promptText="请输入用户名" />
      <TextField fx:id="password1" layoutX="114.0" layoutY="259.0" promptText="请输入密码" />
      <TextField fx:id="password2" layoutX="114.0" layoutY="307.0" promptText="请确认密码" />
      <Button fx:id="validButton" layoutX="114.0" layoutY="374.0" mnemonicParsing="false" onAction="#validation" text="注册" />
      <Button fx:id="exitButton" layoutX="266.0" layoutY="374.0" mnemonicParsing="false" onAction="#exit" text="退出" />
      <ImageView fitHeight="154.0" fitWidth="252.0" layoutX="89.0" layoutY="40.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../image/loginwelcome.png" />
         </image>
      </ImageView>
      <Label layoutX="57.0" layoutY="219.0" text="用户名" />
      <Label layoutX="65.0" layoutY="264.0" text="密码" />
      <Label layoutX="50.0" layoutY="312.0" text="确认密码" />
      <Button fx:id="back" layoutX="153.0" layoutY="421.0" mnemonicParsing="false" onAction="#exit" text="注册成功，退出" visible="false" />
   </children>
</AnchorPane>
